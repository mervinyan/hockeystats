extends layout

block links
    link(href='/stylesheets/c3.css' rel='stylesheet' type='text/css')
    link(href='/stylesheets/style.css' rel='stylesheet' type='text/css')
        
block scripts    
    script(src='/javascripts/d3.min.js' charset='utf-8' type='text/javascript')
    script(src='/javascripts/c3.min.js' type='text/javascript')
    script(src='/javascripts/jquery-2.2.1.min.js' type='text/javascript')    

block content
    h1= title
    p Welcome to #{title} 
    div
        form(name='game_end', method='post' action='/gameentry/gameend')
            div.input
                span.label Stream Id
                input(type='text', name='streamId')
            div.input
                span.label Date
                input(type='text', name='date')
            div.input
                span.label Time
                input(type='text', name='time')
            div.actions
                input(type='submit', value='Save')            


            